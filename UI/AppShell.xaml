<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="UI.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:UI"
    Shell.FlyoutBehavior="Disabled"
    Title="UI">

    <ShellContent
        Title="AdivinarDepartamentos"
        ContentTemplate="{DataTemplate local:AdivinarDepartamentos}"
        Route="AdivinarDepartamentos" />

    <ShellContent
        Title="Resultado"
        ContentTemplate="{DataTemplate local:Resultado}"
        Route="Resultado" />

</Shell>
